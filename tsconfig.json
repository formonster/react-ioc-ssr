{
    "compilerOptions": {
        "checkJs": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "baseUrl": "src",
        "paths": {
            "@/*": [
                "*"
            ],
            "@/store": [
                "shared/store"
            ],
            "@/serviec": [
                "shared/serviec"
            ],
            "@/interface/*": [
                "server/interface/*"
            ]
        },
        "jsx": "react"
    }
}